#!/bin/bash
# {{ ansible_managed }}
cat <<EOF
{
  "daemon": "{{ php_fpm_daemon }}",
  "socket_directory": "{{ php_fpm_socket_directory }}",
  "user": "{{ php_fpm_pool_user }}",
  "group": "{{ php_fpm_pool_group }}",
  "version": {
    "package": "{{ php_package_version }}",
    "major": "{{ php_version[0:1] }}",
    "full": "{{ php_version }}"
  }
}
EOF
